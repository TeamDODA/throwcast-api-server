{
  "name": "throwcast-api-server",
  "version": "1.0.0",
  "description": "podcast app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "NODE_ENV=test mocha -r ./mocha.conf.js -R spec **/*.spec.js",
    "test:integration": "NODE_ENV=test mocha -r ./mocha.conf.js -R spec **/*.integration.js",
    "lint": "npm run lint:tests && npm run lint:scripts",
    "lint:tests": "eslint --ignore-path .eslintignore.test -c .eslintrc.test --ext .js .; exit 0",
    "lint:scripts": "eslint . --ext .js; exit 0",
    "dev": "nodemon index.js",
    "seed": "node ./config/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamDODA/throwcast-api-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TeamDODA/throwcast-api-server/issues"
  },
  "homepage": "https://github.com/TeamDODA/throwcast-api-server#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-things": "^0.2.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "sinon-express-mock": "^1.2.4",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^3.2.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "helmet": "^2.1.2",
    "jsonwebtoken": "^7.1.7",
    "lodash": "^4.14.1",
    "mongoose": "^4.5.8",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "winston": "^2.2.0"
  },
  "engines": {
    "node": "6.3.1"
  }
}
